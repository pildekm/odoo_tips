<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_inventory_control_rules_form" model="ir.ui.view">
		    <field name="name">inventory.control.rules.form</field>
		    <field name="model">stock.warehouse.orderpoint</field>
		    <field name="inherit_id" ref="stock.view_warehouse_orderpoint_form"/>
		    <field name="arch" type="xml">
			     <field name="qty_multiple" position="after">
				     <group string="Season">
					     <field name="season_min_qty"/>
					     <field name="non_season_min_qty"/>
					     <field name="date_start"/>
					     <field name="date_end"/>
					     <field name="date_today"/>
				     </group>
			     </field>
		    </field>
		</record>


	</data>
</odoo>
